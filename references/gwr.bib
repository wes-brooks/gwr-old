	Issn = {1467-9876},
	Journal = {Journal of the Royal Statistical Society: Series C (Applied Statistics)},
	Keywords = {spatial, regression, Generalized linear mixed model, Geostatistics, Kriging, Markov chain Monte Carlo method, Spatial prediction},
	Number = {3},
	Pages = {299--350},
	Publisher = {Blackwell Publishers Ltd.},
	Title = {Model-based geostatistics},
