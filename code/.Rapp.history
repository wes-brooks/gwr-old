    if (any(is.na(weights))) #
        stop("NAs in weights")#
    if (any(weights < 0)) #
        stop("negative weights")#
    y <- model.extract(mf, "response")#
    x <- model.matrix(mt, mf)#
#
    if (is.null(D)) {#
        #Get the matrix of distances#
        n = dim(coords)[1]#
